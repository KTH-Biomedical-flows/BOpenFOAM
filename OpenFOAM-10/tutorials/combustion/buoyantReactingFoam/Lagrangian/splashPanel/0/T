FoamFile
{
    format      ascii;
    location    "0";
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    top
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    region0_to_wallFilm_walls
    {
        type            fixedValue;
        value           uniform 300;
    }
}

