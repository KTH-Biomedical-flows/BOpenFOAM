FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/movingWallRegion";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    plateFaces_top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            inletOutlet;
        inletValue      $internalField;
    }

    atmosphere
    {
        type            inletOutlet;
        inletValue      $internalField;
    }

    region0_to_plateRegion_plateFaces
    {
        type            zeroGradient;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


