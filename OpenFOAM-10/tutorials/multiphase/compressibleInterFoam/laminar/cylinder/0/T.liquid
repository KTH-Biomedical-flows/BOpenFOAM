FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    sides
    {
        type            calculated;
        value           uniform 300;
    }
    top
    {
        type            calculated;
        value           uniform 300;
    }
    frontAndBack
    {
        type            calculated;
        value           uniform 300;
    }
    region0_to_wallFilmRegion_wallFilmFaces
    {
        type            calculated;
        value           uniform 300;
    }
}


