FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mover
{
    type            motionSolver;

    libs            ("libfvMeshMovers.so" "libfvMotionSolvers.so");

    motionSolver    solidBody;

    cellZone        rotor;

    solidBodyMotionFunction  rotatingMotion;

    origin        (0 0 0);
    axis          (0 0 1);
    omega         6.2832; // rad/s
}


